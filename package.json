{
  "name": "code-compass",
  "version": "0.1.0",
  "private": true,
  "description": "Code Compass - AI-first code editor in the browser",
  "scripts": {
    "preflight": "node scripts/preflight.js",
    "healthcheck": "node scripts/healthcheck.js",
    "dev": "node scripts/dev-with-port.js",
    "dev:3001": "next dev --turbopack -p 3001",
    "build": "next build",
    "start": "node scripts/start-with-port.js",
    "lint": "next lint",
    "test": "node scripts/preflight.js && vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "e2e": "playwright test e2e/",
    "e2e:full": "playwright test e2e/full-e2e-report.spec.ts --headed --timeout=600000",
    "e2e:full:ci": "CI=1 playwright test e2e/full-e2e-report.spec.ts --timeout=600000",
    "e2e:validate": "playwright test e2e/output-validation.spec.ts --headed --timeout=900000"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.3",
    "@types/jszip": "^3.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.9.2",
    "js-tiktoken": "^1.0.21",
    "jszip": "^3.10.1",
    "lucide-react": "^0.563.0",
    "next": "15.5.7",
    "next-themes": "^0.4.6",
    "openai": "^6.17.0",
    "prettier": "^3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.58.2",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "msw": "^2.12.10",
    "postcss": "^8",
    "supabase": "^2.74.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@sentry/nextjs": "^10.0.0"
  }
}
